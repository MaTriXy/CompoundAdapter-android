<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.negusoft.compoundadapter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.CompoundAdapter">

        <activity
            android:name=".MainActivity"
            android:label="@string/sample_list_title"
            android:theme="@style/Theme.CompoundAdapter.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".SampleActivity"
            android:label="@string/sample_adapter_group_with_header"
            android:theme="@style/Theme.CompoundAdapter.NoActionBar" />

    </application>

</manifest>